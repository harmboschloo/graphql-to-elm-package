{
  "name": "graphql-to-elm-package",
  "version": "0.2.0",
  "description": "Support package for graphql-to-elm, which generates Elm types, encoders and decoders from GraphQL queries",
  "author": "Harm Boschloo",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/harmboschloo/graphql-to-elm-package",
  "repository": {
    "type": "git",
    "url": "git://github.com/harmboschloo/graphql-to-elm-package.git"
  },
  "keywords": [
    "graphql",
    "elm"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "src"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "elm make",
    "format": "elm-format . --yes",
    "format:validate": "elm-format . --validate",
    "test": "elm-test",
    "prepublishOnly": "yarn format:validate && elm make && elm-test"
  },
  "devDependencies": {
    "elm": "^0.19.0-bugfix6",
    "elm-format": "^0.8.0",
    "elm-test": "^0.19.0-rev4"
  }
}
